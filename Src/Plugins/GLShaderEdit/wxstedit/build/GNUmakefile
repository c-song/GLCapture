ifeq ($(WX_SHARED),0)
../lib/libstedit.a: $(STEDIT_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(STEDIT_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_stedit_lib: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 ../lib/libstedit.a $(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
../lib/libstedit.so: $(STEDIT_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(STEDIT_DLL_OBJECTS) $(LDFLAGS) $(__DEBUGINFO) -pthread -L../lib  $(WX_LIBS)
endif

ifeq ($(WX_SHARED),1)
install_stedit_dll: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 ../lib/libstedit.so $(prefix)/lib
	install -c ../lib/libstedit.so $(prefix)/lib
endif

